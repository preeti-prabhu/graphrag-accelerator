      <data key="d1">An oil tanker for which the building contract is placed after 1 June 1979</data>
      <data key="d2">ed37e6f1bac60538e1edc4c1254b759c</data>
    </node>
    <node id="OIL TANKER DELIVERED BEFORE 6 JULY 1996">
      <data key="d0">VESSEL, MARITIME</data>
