    </edge>
    <edge source="OIL FUEL TANKS" target="ACCIDENTAL OIL FUEL OUTFLOW PERFORMANCE STANDARD">
      <data key="d3">1.0</data>
      <data key="d4">Oil fuel tanks must be constructed and located in a way that complies with the accidental oil fuel outflow performance standard</data>
      <data key="d5">233ba3905d8c199856ef5c2ad5f60d33</data>
    </edge>
    <edge source="ACCIDENTAL OIL FUEL OUTFLOW PERFORMANCE STANDARD" target="OIL FUEL LEVEL AFTER DAMAGE">
