      <data key="d5">c25139d45688b46490177cf04dab5e03</data>
    </edge>
    <edge source="BARGE" target="PORT OF NEW ORLEANS">
      <data key="d3">1.0</data>
      <data key="d4">The Port of New Orleans may use barges to transport cargo to and from shallow-draft waterways</data>
      <data key="d5">13025dc23fae9aab7db03deb125f4f33</data>
    </edge>
